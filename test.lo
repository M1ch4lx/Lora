import Vector as vec;

function sum(array: Array): Number
{
    result = 0;
    for(e in array)
    {
        result = result + e;
    }
    return result;
}

test_vec: vec = vec.zero();

print("Printing test vec");
test_vec.print();

print(sum([1,2,3]));

callback: Callback = sum;

result = callback([1,2,4]);

print(result);

